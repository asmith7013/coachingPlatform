    "@types/node": "^20.17.23",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "9",
    "eslint-config-next": "15.2.0",
    "next": "^15.2.4",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "0.6.11",
    "tailwindcss": "4.0.17",
    "tsx": "^4.19.3",
    "typescript": "5.7.3"
  }
