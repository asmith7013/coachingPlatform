{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run build-theme && npm run generate-tree && next dev",
    "build": "npm run build-theme && next build",
    "build-theme": "tsx scripts/generate-theme-css.ts",
    "start": "next start",
    "lint": "next lint",
    "generate-tree": "tree -I 'node_modules|dist|build|logs|coverage|.git|.next|.vercel|.vscode|.idea|.DS_Store|pnpm-lock.yaml|package-lock.json|yarn.lock|*.env|*.log|*.cache' -L 6 > project-tree.txt",
    "generate-safelist": "tsx scripts/generateSafelist.ts"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^4.1.3",
    "@sentry/nextjs": "^9.5.0",
    "@tiptap/pm": "2.11.5",
    "@tiptap/react": "2.11.5",
    "@tiptap/starter-kit": "2.11.5",
    "@typegoose/typegoose": "12.13.0",
    "axios": "^1.8.1",
    "chart.js": "^4.4.8",
    "clsx": "^2.1.1",
    "framer-motion": "12.4.7",
    "mongoose": "^8.12.1",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.5.0",
